<?xml version="1.0" encoding="UTF-8"?>
<mealyMachine>
    <state id="a">
        <transition to="b" input="x" output="1"/>
        <transition to="c" input="y" output="0"/>
        <transition to="c" input="z" output="1"/>
    </state>
    <state id="b">
        <transition to="c" input="x, y, z" output="1"/>
    </state>
    <state id="c">
        <transition to="a" input="x, y, z" output="1"/>
    </state>
</mealyMachine>
